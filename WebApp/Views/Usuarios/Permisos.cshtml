@model List<Entidades.Menu>



<table class="table table-bordered" id="ListaPaises" width="100%" cellspacing="0">
    <thead>
        <tr>
            <th>#</th>
            <th>Accion</th>
            <th>Activo</th>
            <th>Descripcion</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.IdMenu</td>
            <td>@item.Accion</td>
            <td>@item.Activo</td>
            <td>@item.Descripcion</td>
        </tr>
        }
    </tbody>
</table>